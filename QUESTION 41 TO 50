SOL 41- 
#include<stdio.h>
#include<math.h>
int main(){
int n,temp,d,ld,fd,newn;
scanf("%d",&n);
temp=n;
ld=n%10;
d=(int)log10(n);
fd=n/(int)pow(10,d);
newn=ld*(int)pow(10,d)+n%((int)pow(10,d))-ld+fd;
printf("%d",newn);
return 0;
}
SOL 42-
#include<stdio.h>
int main(){
int n,i,sum=0;
scanf("%d",&n);
for(i=1;i<n;i++){
if(n%i==0) sum+=i;
}
if(sum==n) printf("Perfect Number");
else printf("Not Perfect Number");
return 0;

SOL43-

#include<stdio.h>
int main(){
int n,temp,rem,sum=0,f,i;
scanf("%d",&n);
temp=n;
while(temp!=0){
rem=temp%10;
f=1;
for(i=1;i<=rem;i++) f*=i;
sum+=f;
temp/=10;
}
if(sum==n) printf("Strong Number");
else printf("Not Strong Number");
return 0;
}

SOL 44-

#include<stdio.h>
int main(){
int n;
float sum=0;
scanf("%d",&n);
for(int i=1,j=2,k=1;i<=n;i++,j+=2,k+=2)
sum+= (float)j/k;
printf("%.2f",sum);
return 0;
}

SOL 45-
#include<stdio.h>
int main(){
int n;
float sum=0;
scanf("%d",&n);
int num=2,den=3;
for(int i=1;i<=n;i++){
sum+= (float)num/den;
num+=2;
den+=4;
}
printf("%.2f",sum);
return 0;
}

SOL 46-
#include<stdio.h>
int main(){
int i,j;
for(i=1;i<=5;i++){
for(j=1;j<=5;j++)
printf("*");
printf("\n");
}
return 0;
}

