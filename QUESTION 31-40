sol 31 
#include<stdio.h>
int main(){
int n,i;
unsigned int mask;
scanf("%d",&n);
for(i=31;i>=0;i--){
mask=1<<i;
printf("%d",(n&mask)?1:0);
}
return 0;
}
sol 32-
#include<stdio.h>
int main(){
int n,rev=0,rem,temp;
scanf("%d",&n);
temp=n;
while(n!=0){
rem=n%10;
rev=rev*10+rem;
n/=10;
}
if(temp==rev)
printf("Palindrome");
else
printf("Not Palindrome");
return 0;
}
SOL-33
#include<stdio.h>
#include<math.h>
int main(){
int n,temp,rem,digits=0;
double sum=0;
scanf("%d",&n);
temp=n;
while(temp!=0){
temp/=10;
digits++;
}
temp=n;
while(temp!=0){
rem=temp%10;
sum+=pow(rem,digits);
temp/=10;
}
if((int)sum==n)
printf("Armstrong");
else
printf("Not Armstrong");
return 0;
}

SOL34- 
#include<stdio.h>
int main(){
int n,i,flag=1;
scanf("%d",&n);
if(n<=1) flag=0;
for(i=2;i*i<=n;i++){
if(n%i==0){
flag=0;
break;
}
}
if(flag) printf("Prime");
else printf("Not Prime");
return 0;
}

SOL35 - 
#include<stdio.h>
int main(){
int n,i;
scanf("%d",&n);
for(i=1;i<=n;i++){
if(n%i==0)
printf("%d ",i);
}
return 0;
}

SOL36- 
#include<stdio.h>
int main(){
int a,b,temp;
scanf("%d%d",&a,&b);
while(b!=0){
temp=b;
b=a%b;
a=temp;
}
printf("%d",a);
return 0; 
}

SOL 37 -
#include<stdio.h>
int main(){
int a,b,hcf,temp;
scanf("%d%d",&a,&b);
int x=a,y=b;
while(b!=0){
temp=b;
b=a%b;
a=temp;
}
hcf=a;
printf("%d",(x*y)/hcf);
return 0;
}

SOL 38 - 
#include<stdio.h>
int main(){
int n,sum=0,rem;
scanf("%d",&n);
while(n!=0){
rem=n%10;
sum+=rem;
n/=10;
}
printf("%d",sum);
return 0;
}

SOL 39-
#include<stdio.h>
int main(){
int n,rem,prod=1,flag=0;
scanf("%d",&n);
while(n!=0){
rem=n%10;
if(rem%2!=0){
prod*=rem;
flag=1;
}
n/=10;
}
if(flag) printf("%d",prod);
else printf("No odd digits");
return 0;
}

SOL 40-
#include<stdio.h>
#include<string.h>
int main(){
char b[100];
scanf("%s",b);
for(int i=0;b[i]!='\0';i++){
if(b[i]=='0') b[i]='1';
else if(b[i]=='1') b[i]='0';
else{printf("Invalid binary");return 0;}
}
printf("%s",b);
return 0;
}



